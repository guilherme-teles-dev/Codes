algoritmo "semnome"
var
   cont, fib1, fib2, fib3: inteiro
inicio
      cont <- 1
      fib3 <- 1
      enquanto (cont <= 14) faca
               escreval(fib3)
               fib1 <- fib2
               fib2 <- fib3
               fib3 <- fib1 + fib2
               cont <- cont + 1
      Fimenquanto
fimalgoritmo
